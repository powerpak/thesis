issn = {0378-1097},
journal = {FEMS Microbiol. Lett.},
keywords = {Bacterial Infections,Bacterial Infections: epidemiology,Bacteriophages,Bacteriophages: genetics,Bacteriophages: isolation {\&} purification,Bacteriophages: pathogenicity,Blotting, Southern,California,Cholera Toxin,Cholera Toxin: genetics,DNA, Bacterial,DNA, Bacterial: analysis,DNA, Bacterial: isolation {\&} purification,Diphtheria Toxin,Diphtheria Toxin: genetics,Enterotoxins,Enterotoxins: genetics,Environmental Monitoring,Epidemiological Monitoring,Exotoxins,Exotoxins: genetics,Genes, Bacterial,Polymerase Chain Reaction,Polymerase Chain Reaction: methods,Shiga Toxin,Shiga Toxin: genetics,Soil Microbiology,Staphylococcus,Staphylococcus: genetics,Water Microbiology},
