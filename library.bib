@article{Korves2009,
abstract = {Knowledge about pathogenesis is increasing dramatically, and most of this information is stored in the scientific literature or in sequence databases. This information can be made more accessible by the use of ontologies or controlled vocabularies. Recently, several ontologies, controlled vocabularies and databases have been developed or adapted for virulence factors and their roles in pathogenesis. Here, we discuss these systems, how they are being used in research and the challenges that remain for developing and applying ontologies for virulence factors.},
