pages = {1435--49},
pmid = {20923983},
title = {{Inference of homologous recombination in bacteria using whole-genome sequences.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2998322{\&}tool=pmcentrez{\&}rendertype=abstract},
