doi = {10.1016/j.antiviral.2008.05.012},
file = {:Users/powerpak/Documents/Mendeley Desktop/Boltz et al. - 2008 - Intramuscularly administered neuraminidase inhibitor peramivir is effective against lethal H5N1 influenza virus in.pdf:pdf},
journal = {Antiviral Res.},
number = {2},
pages = {150--157},
pmid = {18573280},
title = {{Intramuscularly administered neuraminidase inhibitor peramivir is effective against lethal H5N1 influenza virus in mice.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=18573280{\&}retmode=ref{\&}cmd=prlinks papers2://publication/doi/10.1016/j.antiviral.2008.05.012},
volume = {80},
