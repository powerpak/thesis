isbn = {1471-2105},
issn = {1471-2105},
journal = {BMC Bioinformatics},
pages = {304},
pmid = {20525342},
title = {{Scaffold filling, contig fusion and comparative gene order inference.}},
volume = {11},
year = {2010}
}
