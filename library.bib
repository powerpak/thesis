doi = {10.1101/gr.074914.107},
file = {:Users/powerpak/Documents/Mendeley Desktop/Keller et al. - 2008 - A gene expression network model of type 2 diabetes links cell cycle regulation in islets with diabetes susceptibi.pdf:pdf},
issn = {1088-9051},
journal = {Genome Res.},
