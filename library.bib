issn = {1537-6605},
journal = {Am. J. Hum. Genet.},
keywords = {Case-Control Studies,Cohort Studies,Diabetes Mellitus, Type 2,Diabetes Mellitus, Type 2: genetics,Diabetes Mellitus, Type 2: pathology,Gene Expression Profiling,Genome-Wide Association Study,Genotype,Humans,Meta-Analysis as Topic,Oligonucleotide Array Sequence Analysis,Phenotype,Polymorphism, Single Nucleotide,Polymorphism, Single Nucleotide: genetics,Signal Transduction,Signal Transduction: genetics},
month = {apr},
number = {4},
pages = {581--91},
pmid = {20346437},
