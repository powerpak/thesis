file = {:Users/powerpak/Documents/Mendeley Desktop/Nascimento et al. - 2009 - Gene expression profiling during early acute febrile stage of dengue infection can predict the disease outcom.pdf:pdf},
issn = {1932-6203},
journal = {PLoS One},
keywords = {Adult,Aged,Aged, 80 and over,Apoptosis,Biological Markers,Biological Markers: metabolism,Brazil,Cohort Studies,Dengue,Dengue Virus,Dengue Virus: genetics,Dengue Virus: metabolism,Dengue: diagnosis,Dengue: genetics,Dengue: metabolism,Female,Fever,Fever: diagnosis,Fever: metabolism,Gene Expression Regulation,Humans,Male,Middle Aged,Multivariate Analysis,Oligonucleotide Array Sequence Analysis,Treatment Outcome},
