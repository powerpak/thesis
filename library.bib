author = {Li, Heng and Durbin, Richard},
doi = {10.1093/bioinformatics/btp698},
file = {:Users/powerpak/Documents/Mendeley Desktop/Li, Durbin - 2010 - Fast and accurate long-read alignment with Burrows-Wheeler transform.pdf:pdf},
