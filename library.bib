title = {{Integrating pathway analysis and genetics of gene expression for genome-wide association studies.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2850442{\&}tool=pmcentrez{\&}rendertype=abstract},
