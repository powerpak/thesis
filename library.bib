author = {Klann, Jeffrey G and Buck, Michael D and Brown, Jeffrey and Hadley, Marc and Elmore, Richard and Weber, Griffin M and Murphy, Shawn N},
doi = {10.1136/amiajnl-2014-002707},
file = {:Users/powerpak/Documents/Mendeley Desktop/Klann et al. - 2014 - Query Health standards-based, cross-platform population health surveillance.pdf:pdf},
isbn = {1067-5027},
issn = {1527-974X},
journal = {J. Am. Med. Inform. Assoc.},
number = {4},
pages = {650--6},
pmid = {24699371},
title = {{Query Health: standards-based, cross-platform population health surveillance.}},
