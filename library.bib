file = {:Users/powerpak/Documents/Mendeley Desktop/Lo et al. - 2014 - Improving the work efficiency of healthcare-associated infection surveillance using electronic medical records.pdf:pdf},
issn = {1872-7565},
journal = {Comput. Methods Programs Biomed.},
keywords = {electronic medical records,healthcare-associated infection,urinary tract infection},
month = {nov},
number = {2},
pages = {351--9},
pmid = {25154644},
publisher = {Elsevier Ireland Ltd},
title = {{Improving the work efficiency of healthcare-associated infection surveillance using electronic medical records.}},
