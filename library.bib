number = {table 1},
pages = {1602--1609},
pmid = {18190323},
title = {{Stenotrophomonas maltophilia: changing spectrum of a serious bacterial pathogen in patients with cancer.}},
volume = {45},
year = {2007}
}
@article{Li2013,
abstract = {BACKGROUND: Sample size calculation is an important issue in the experimental design of biomedical research. For RNA-seq experiments, the sample size calculation method based on the Poisson model has been proposed; however, when there are biological replicates, RNA-seq data could exhibit variation significantly greater than the mean (i.e. over-dispersion). The Poisson model cannot appropriately model the over-dispersion, and in such cases, the negative binomial model has been used as a natural extension of the Poisson model. Because the field currently lacks a sample size calculation method based on the negative binomial model for assessing differential expression analysis of RNA-seq data, we propose a method to calculate the sample size.$\backslash$n$\backslash$nRESULTS: We propose a sample size calculation method based on the exact test for assessing differential expression analysis of RNA-seq data.$\backslash$n$\backslash$nCONCLUSIONS: The proposed sample size calculation method is straightforward and not computationally intensive. Simulation studies to evaluate the performance of the proposed sample size method are presented; the results indicate our method works well, with achievement of desired power.},
annote = {{\{}:PMCID:PMC3924199{\}}},
author = {Li, Chung-I and Su, Pei-Fang and Shyr, Yu},
doi = {10.1186/1471-2105-14-357},
file = {:Users/powerpak/Documents/Mendeley Desktop/Li, Su, Shyr - 2013 - Sample size calculation based on exact test for assessing differential expression analysis in RNA-seq data.pdf:pdf},
issn = {1471-2105},
journal = {BMC Bioinformatics},
keywords = {Base Sequence,Computer Simulation,Computer Simulation: statistics {\&} numerical data,Gene Expression Regulation,Likelihood Functions,Models, Statistical,Poisson Distribution,RNA,RNA: antagonists {\&} inhibitors,RNA: biosynthesis,RNA: genetics,Random Allocation,Research Design,Research Design: statistics {\&} numerical data,Research Design: trends,Sample Size,Sequence Analysis, RNA,Sequence Analysis, RNA: methods,Sequence Analysis, RNA: statistics {\&} numerical dat,Sequence Analysis, RNA: trends,User-Computer Interface},
pages = {357},
pmid = {24314022},
title = {{Sample size calculation based on exact test for assessing differential expression analysis in RNA-seq data.}},
