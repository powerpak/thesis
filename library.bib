doi = {10.1128/AAC.02555-14},
file = {:Users/powerpak/Documents/Mendeley Desktop/Cannatelli et al. - 2014 - In vivo evolution to Colistin resistance by PmrB sensor kinase mutation in KPC-producing Klebsiella pneumonia.pdf:pdf},
issn = {10986596},
journal = {Antimicrob. Agents Chemother.},
number = {8},
pages = {4399--4403},
pmid = {24841267},
title = {{In vivo evolution to Colistin resistance by PmrB sensor kinase mutation in KPC-producing Klebsiella pneumoniae is associated with low-dosage colistin treatment}},
volume = {58},
year = {2014}
}
