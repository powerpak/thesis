issn = {1234-1010},
journal = {Med. Sci. Monit.},
keywords = {Alzheimer Disease,Alzheimer Disease: enzymology,Alzheimer Disease: metabolism,Alzheimer Disease: prevention {\&} control,Amyloid Precursor Protein Secretases,Amyloid beta-Peptides,Amyloid beta-Peptides: metabolism,Aspartic Acid Endopeptidases,Aspartic Acid Endopeptidases: genetics,Cell Line,Endopeptidases,Endopeptidases: genetics,Enzymologic,Enzymologic: physiology,Gene Expression Regulation,Humans,Messenger,Messenger: genetics,Morphine,Morphine: pharmacology,Nitric Oxide,Nitric Oxide: physiology,RNA,Reverse Transcriptase Polymerase Chain Reaction,Tumor},
