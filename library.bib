doi = {10.1073/pnas.0705362104},
file = {:Users/powerpak/Documents/Mendeley Desktop/Dickerson et al. - 2007 - Phage escape libraries for checkmate analysis.pdf:pdf},
journal = {Proc. Natl. Acad. Sci. U. S. A.},
number = {31},
pages = {12703--12708},
pmid = {17646645},
title = {{Phage escape libraries for checkmate analysis.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=17646645{\&}retmode=ref{\&}cmd=prlinks papers2://publication/doi/10.1073/pnas.0705362104},
