journal = {BMC Genomics},
keywords = {Computer Graphics,Genomics,Genomics: methods,High-Throughput Nucleotide Sequencing,High-Throughput Nucleotide Sequencing: methods,Information Dissemination,Internet,Phylogeny,Publications,Statistics as Topic,Statistics as Topic: methods},
number = {1},
