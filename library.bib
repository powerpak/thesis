isbn = {1879-0542 (Electronic)$\backslash$r0165-2478 (Linking)},
issn = {01652478},
journal = {Immunol. Lett.},
keywords = {CD14,CD16,Human,Monocyte,Phenotype},
number = {1},
pages = {32--41},
pmid = {23557598},
publisher = {Elsevier B.V.},
title = {{Sources of heterogeneity in human monocyte subsets}},
url = {http://dx.doi.org/10.1016/j.imlet.2013.03.004},
volume = {152},
year = {2013}
}
