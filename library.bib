journal = {Cochrane database Syst. Rev.},
keywords = {Cystic Fibrosis,Cystic Fibrosis: complications,Cystic Fibrosis: microbiology,Gram-Negative Bacterial Infections,Gram-Negative Bacterial Infections: drug therapy,Humans,Respiratory Tract Infections,Respiratory Tract Infections: drug therapy,Respiratory Tract Infections: microbiology,Stenotrophomonas maltophilia},
