journal = {Arch. Virol.},
number = {1-2},
pages = {29--37},
title = {{Studies on the genetic basis of human influenza A virus adaptation to mice: degrees of virulence of reassortants with defined genetic content.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=2719553{\&}retmode=ref{\&}cmd=prlinks papers2://publication/uuid/894A32BB-C968-477F-B9E3-EDEE4E758FDE},
