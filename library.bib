issn = {1460-2091},
journal = {J. Antimicrob. Chemother.},
keywords = {Bacteria,Bacteria: drug effects,Bacteria: genetics,Bacterial,Bacterial Infections,Bacterial Infections: microbiology,Computational Biology,Computational Biology: methods,Drug Resistance,Humans,Internet},
month = {nov},
