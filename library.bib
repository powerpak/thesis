number = {6},
pages = {403--410},
pmid = {19394704},
publisher = {Elsevier Ltd},
title = {{Recurrent Clostridium difficile infection: A review of risk factors, treatments, and outcomes}},
url = {http://dx.doi.org/10.1016/j.jinf.2009.03.010},
volume = {58},
year = {2009}
}
