file = {:Users/powerpak/Documents/Mendeley Desktop/Afshinnekoo et al. - 2015 - Geospatial Resolution of Human and Bacterial Diversity with City-Scale Metagenomics Article Geospatial Resol.pdf:pdf},
isbn = {2405-4712},
