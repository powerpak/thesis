journal = {Epidemiol. Infect.},
number = {6},
pages = {1010--3},
pmid = {17156501},
title = {{Accuracy of ICD-9 coding for Clostridium difficile infections: a retrospective cohort.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17156501{\%}5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2870650},
volume = {135},
year = {2007}
}
