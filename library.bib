doi = {10.1038/nm1201-1306},
file = {:Users/powerpak/Documents/Mendeley Desktop/Chen et al. - 2001 - A novel influenza A virus mitochondrial protein that induces cell death.pdf:pdf},
journal = {Nat. Med.},
number = {12},
pages = {1306--1312},
