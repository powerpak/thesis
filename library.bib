doi = {10.1098/rsif.2013.0414},
file = {:Users/powerpak/Documents/Mendeley Desktop/Reich et al. - 2013 - Interactions between serotypes of dengue highlight epidemiological impact of cross-immunity.pdf:pdf},
isbn = {1742-5662 (Electronic)$\backslash$r1742-5662 (Linking)},
issn = {1742-5662},
journal = {J. R. Soc. Interface},
keywords = {Animals,Cross Protection,Cross Reactions,Cross Reactions: immunology,Culicidae,Culicidae: virology,Dengue,Dengue Virus,Dengue Virus: immunology,Dengue: epidemiology,Dengue: genetics,Dengue: virology,Female,Humans,Immunologic Memory,Male,Models, Immunological,Retrospective Studies,Thailand,Thailand: epidemiology},
