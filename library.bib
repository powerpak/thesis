@article{Valdezate2005,
abstract = {OBJECTIVES: To ascertain the participation of topoisomerase mutations in the development of ciprofloxacin resistance in isogenic Stenotrophomonas maltophilia mutants. METHODS: gyrAB and parCE sequences in three paired in vivo isogenic ciprofloxacin-susceptible (MIC range 0.5-4 mg/L) and resistant (16-128 mg/L) S. maltophilia strains (PFGE-characterized) sequentially isolated from three patients, and their corresponding in vitro mutants (ciprofloxacin MIC range 2-{\textgreater}128 mg/L), were studied. Efflux phenotype was also investigated. RESULTS: Despite different quinolone susceptibilities, each paired clinical strain displayed identical gyrAB and parCE sequences as well as their corresponding in vitro mutants. Up to 50{\%} (18/36) of in vitro mutants displayed a positive efflux phenotype when nalidixic acid was combined with MC-207,110, while 6{\%} (2/36) showed the phenotype when exposed to nalidixic acid and reserpine. Carbonyl cyanide m-chlorophenylhydrazone or arsenite failed to alter quinolone MICs. CONCLUSIONS: The increase of ciprofloxacin MICs in in vivo and in vitro isogenic S. maltophilia mutant strains was not related to quinolone resistance determining region mutations. Highly effective efflux mechanisms might preserve topoisomerase targets from a ciprofloxacin challenge in S. maltophilia.},
author = {Valdezate, Sylvia and Vindel, Ana and Sa{\'{e}}z-Nieto, Juan Antonio and Baquero, Fernando and Cant{\'{o}}n, Rafael},
doi = {10.1093/jac/dki182},
file = {:Users/powerpak/Documents/Mendeley Desktop/Valdezate et al. - 2005 - Preservation of topoisomerase genetic sequences during in vivo and in vitro development of high-level resistan.pdf:pdf},
isbn = {3491336880},
issn = {03057453},
journal = {J. Antimicrob. Chemother.},
keywords = {Efflux pump inhibitors,Fluoroquinolone resistance,QRDR},
number = {1},
pages = {220--223},
pmid = {15928010},
title = {{Preservation of topoisomerase genetic sequences during in vivo and in vitro development of high-level resistance to ciprofloxacin in isogenic Stenotrophomonas maltophilia strains}},
