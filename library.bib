keywords = {Gene Expression Profiling,Genome, Bacterial,Genotype,High-Throughput Nucleotide Sequencing,Microbiological Techniques,Microbiological Techniques: methods,Phenotype,Sequence Analysis, DNA,Sequence Analysis, RNA},
