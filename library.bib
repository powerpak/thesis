isbn = {0924-8579},
issn = {09248579},
journal = {Int. J. Antimicrob. Agents},
keywords = {Biofilm formation,Efflux pumps,Integrons,Melanin,Multidrug resistance,Phosphoglucomutase (SpgM),Stenotrophomonas maltophilia},
pages = {126--130},
pmid = {19926255},
title = {{Multidrug resistance in clinical isolates of Stenotrophomonas maltophilia: roles of integrons, efflux pumps, phosphoglucomutase (SpgM), and melanin and biofilm formation}},
