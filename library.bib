issn = {1098-6596},
journal = {Antimicrob. Agents Chemother.},
keywords = {Computational Biology,Computational Biology: methods,Databases, Genetic,Drug Resistance, Microbial,Drug Resistance, Microbial: genetics,Genome, Bacterial,Genome, Bacterial: genetics,Software},
month = {jan},
number = {1},
