file = {:Users/powerpak/Documents/Mendeley Desktop/Hanson et al. - 2006 - Passive immunoprophylaxis and therapy with humanized monoclonal antibody specific for influenza A H5 hemagglutini.pdf:pdf},
journal = {Respir. Res.},
pages = {126},
pmid = {17040574},
title = {{Passive immunoprophylaxis and therapy with humanized monoclonal antibody specific for influenza A H5 hemagglutinin in mice.}},
