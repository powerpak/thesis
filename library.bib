@article{Miner2015,
abstract = {OBJECTIVE: Chikungunya virus (CHIKV) is an arthritogenic mosquito-transmitted alphavirus that spread to the Caribbean in 2013 and to the US in 2014. CHIKV-infected patients develop inflammatory arthritis that can persist for months or years, but little is known about the rheumatologic and immunologic features of CHIKV-related arthritis in humans, particularly as compared to rheumatoid arthritis (RA). The purpose of this study was to describe these features in a group of 10 American travelers who were nearly simultaneously infected while visiting Haiti in June 2014.$\backslash$n$\backslash$nMETHODS: Patient history was obtained and physical examination and laboratory tests were performed. All patients with CHIKV-related arthritis had detectable levels of anti-CHIKV IgG. Using cytometry by time-of-flight (CyTOF), we analyzed peripheral blood mononuclear cells in CHIKV-infected patients, healthy controls, and patients with untreated, active RA.$\backslash$n$\backslash$nRESULTS: Among 10 CHIKV-infected individuals, 8 developed persistent symmetric polyarthritis that met the American College of Rheumatology/European League Against Rheumatism 2010 criteria for (seronegative) RA. CyTOF analysis revealed that RA and CHIKV-infected patients had greater percentages of activated and effector CD4+ and CD8+ T cells than healthy controls.$\backslash$n$\backslash$nCONCLUSION: In addition to similar clinical features, patients with CHIKV infection and patients with RA develop very similar peripheral T cell phenotypes. These overlapping clinical and immunologic features highlight a need for rheumatologists to consider CHIKV infection when evaluating patients with new, symmetric polyarthritis.},
author = {Miner, Jonathan J. and Yeang, Han Xian Aw and Fox, Julie M. and Taffner, Samantha and Malkova, Olga N. and Oh, Stephen T. and Kim, Alfred H J and Diamond, Michael S. and Lenschow, Deborah J. and Yokoyama, Wayne M.},
doi = {10.1002/art.39027},
file = {:Users/powerpak/Documents/Mendeley Desktop/Miner et al. - 2015 - Brief report Chikungunya viral arthritis in the United States A mimic of seronegative rheumatoid arthritis.pdf:pdf;:Users/powerpak/Documents/Mendeley Desktop/Miner et al. - 2015 - Brief report Chikungunya viral arthritis in the United States A mimic of seronegative rheumatoid arthritis.docx:docx},
isbn = {8133815541},
issn = {23265205},
journal = {Arthritis Rheumatol.},
number = {5},
pages = {1214--1220},
pmid = {25605621},
title = {{Brief report: Chikungunya viral arthritis in the United States: A mimic of seronegative rheumatoid arthritis}},
volume = {67},
year = {2015}
}
