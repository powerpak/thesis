file = {:Users/powerpak/Documents/Mendeley Desktop/Tasan et al. - 2008 - An en masse phenotype and function prediction system for Mus musculus.pdf:pdf},
journal = {Genome Biol.},
pages = {S8},
pmid = {18613952},
title = {{An en masse phenotype and function prediction system for Mus musculus.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=18613952{\&}retmode=ref{\&}cmd=prlinks papers2://publication/doi/10.1186/gb-2008-9-s1-s8},
