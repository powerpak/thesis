file = {:Users/powerpak/Documents/Mendeley Desktop/Veiga-Castelli et al. - 2010 - Genomic alterations detected by comparative genomic hybridization in ovarian endometriomas.pdf:pdf},
isbn = {1414-431X (Electronic)$\backslash$r0100-879X (Linking)},
issn = {0100879X},
journal = {Brazilian J. Med. Biol. Res.},
keywords = {Chromosomal imbalances,Comparative genomic hybridization,Endometrioma,Endometriosis},
number = {8},
pages = {799--805},
pmid = {20725696},
title = {{Genomic alterations detected by comparative genomic hybridization in ovarian endometriomas}},
volume = {43},
