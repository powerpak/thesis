file = {:Users/powerpak/Documents/Mendeley Desktop/Sun et al. - 2012 - Dynamic population changes in mycobacterium tuberculosis during acquisition and fixation of drug resistance in patie.pdf:pdf},
isbn = {1537-6613 (Electronic)$\backslash$r0022-1899 (Linking)},
issn = {00221899},
journal = {J. Infect. Dis.},
pages = {1724--1733},
pmid = {22984115},
title = {{Dynamic population changes in mycobacterium tuberculosis during acquisition and fixation of drug resistance in patients}},
volume = {206},
year = {2012}
}
