issn = {01956701},
journal = {J. Hosp. Infect.},
keywords = {Clostridium difficile,Disinfection,Infection control Interventions,Nosocomial infection,Statistical model},
number = {3},
pages = {227--234},
pmid = {23787167},
publisher = {Elsevier Ltd},
title = {{Impact of cleaning and other interventions on the reduction of hospital-acquired Clostridium difficile infections in two hospitals in England assessed using a breakpoint model}},
