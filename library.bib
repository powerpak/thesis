doi = {10.1128/JVI.02742-09},
file = {:Users/powerpak/Documents/Mendeley Desktop/Belser et al. - 2010 - Pathogenesis of pandemic influenza A (H1N1) and triple-reassortant swine influenza A (H1) viruses in mice.pdf:pdf},
