journal = {J. Immunol.},
keywords = {*Immunity,Acute Disease,Alphavirus Infections/blood/*immunology,Animal,Animals,B-Lymphocyte Subsets/immunology/virology,Cells,Cercopithecus aethiops,Chikungunya virus/*immunology,Chronic Disease,Cultured,Dendritic Cells/immunology/virology,Disease Models,Humans,Innate,Insect Vectors/immunology/virology,Macaca,Macrophages/immunology/virology,Monocytes/*immunology/secretion/*virology,Myeloid Cells/immunology/virology,Vero Cells,Viremia/immunology},
