@article{Doi2014,
abstract = {The whole genome sequence of a carbapenem-resistant Klebsiella pneumoniae strain PittNDM01 co-producing NDM-1 and OXA-232 carbapenemases was determined. The use of a single molecule, real-time (SMRT) sequencing provided a closed genome in a single sequencing run. K. pneumoniae PittNDM01 had a single chromosome of 5,348,284 bp and four plasmids, pPKPN1(283,371 bp), pPKPN2 (103,694 bp), pPKPN3 (70,814 bp), and pPKPN4 (6,141 bp). The contents of the chromosome were similar to that of the K. pneumoniae reference genome MGH 78578, with the exception of a large inversion spanning 23.3{\%} of the chromosome. In contrast, three of the four plasmids were unique. The plasmid pPKPN1, an IncHI1B-like plasmid, encoded blaNDM-1, armA and qnrB1 along with tellurium and mercury resistance operons. blaNDM-1 was carried on a unique structure where Tn125 was further bracketed by IS26 downstream of a class 1 integron. IncFIA-like plasmid pPKPN3 also encoded an array of resistance elements including blaCTX-M-15 and a mercury resistance operon. ColE-type pPKPN4 encoding blaOXA-232 was identical to a plasmid previously reported from France. SMRT sequencing was useful in resolving complex bacterial genomic structures in de novo assemblies.},
annote = {{\{}:PMCID:PMC4187971{\}}},
author = {Doi, Yohei and Hazen, Tracy H and Boitano, Matthew and Tsai, Yu-Chih and Clark, Tyson A and Korlach, Jonas and Rasko, David A},
doi = {10.1128/AAC.03180-14},
file = {:Users/powerpak/Documents/Mendeley Desktop/Doi et al. - 2014 - Whole Genome Assembly of Klebsiella pneumoniae Co-Producing NDM-1 and OXA-232 Carbapenemases Using Single-Molecule,.pdf:pdf},
issn = {1098-6596},
journal = {Antimicrob. Agents Chemother.},
number = {10},
pages = {AAC.03180--14--},
pmid = {25070096},
title = {{Whole Genome Assembly of Klebsiella pneumoniae Co-Producing NDM-1 and OXA-232 Carbapenemases Using Single-Molecule, Real-Time Sequencing.}},
url = {http://aac.asm.org/content/early/2014/07/22/AAC.03180-14.abstract?sid=02bfc701-d8a9-4d3b-a390-e441caf3008f},
volume = {58},
