journal = {Gastroenterology},
keywords = {idweek},
mendeley-tags = {idweek},
number = {4},
pages = {1206--1214},
pmid = {19162027},
publisher = {AGA Institute American Gastroenterological Association},
title = {{Prospective Derivation and Validation of a Clinical Prediction Rule for Recurrent Clostridium difficile Infection}},
