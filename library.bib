@article{Beyersmann2006,
abstract = {BACKGROUND: Reliable data on the costs attributable to nosocomial infection (NI) are crucial to demonstrating the real cost-effectiveness of infection control measures. Several studies investigating this issue with regard to intensive care unit (ICU) patients have probably overestimated, as a result of inappropriate study methods, the part played by NIs in prolonging the length of stay.$\backslash$n$\backslash$nMETHODS: Data from a prospective study of the incidence of NI in 5 ICUs over a period of 18 months formed the basis of this analysis. For describing the temporal dynamics of the data, a multistate model was used. Thus, ICU patients were counted as case patients as soon as an NI was ascertained on any particular day. All patients were then regarded as control subjects as long as they remained free of NI (time-to-event data analysis technique).$\backslash$n$\backslash$nRESULTS: Admitted patients (n=1,876) were observed for the development of NI over a period of 28,498 patient-days. In total, 431 NIs were ascertained during the study period (incidence density, 15.1 NIs per 1,000 patient-days). The influence of NI as a time-dependent covariate in a proportional hazards model was highly significant (P{\textless} .0001, Wald test). NI significantly reduced the discharge hazard (hazard ratio, 0.72 [95{\%} confidence interval, 0.63-0.82])--that is, it prolonged the ICU stay. The mean prolongation of ICU length of stay due to NI (+/- standard error) was estimated to be 5.3+/-1.6 days.$\backslash$n$\backslash$nCONCLUSIONS: Further studies are required to enable comparison of data on prolongation of ICU length of stay with the results of various study methods.},
author = {Beyersmann, J and Gastmeier, P and Grundmann, H and B{\"{a}}rwolff, S and Geffers, C and Behnke, M and R{\"{u}}den, H and Schumacher, M},
doi = {10.1086/503375},
file = {:Users/powerpak/Documents/Mendeley Desktop/Beyersmann et al. - 2006 - Use of multistate models to assess prolongation of intensive care unit stay due to nosocomial infection.pdf:pdf},
isbn = {0899-823X (Print)$\backslash$r0899-823x},
issn = {0899-823X},
journal = {Infect. Control Hosp. Epidemiol.},
keywords = {Aged,Bacteremia,Bacteremia: epidemiology,Bacteremia: mortality,Cross Infection,Cross Infection: epidemiology,Cross Infection: mortality,Female,Hospitals, University,Humans,Incidence,Infection Control,Infection Control: methods,Intensive Care Units,Length of Stay,Male,Middle Aged,Models, Biological,Pneumonia,Pneumonia: epidemiology,Pneumonia: mortality,Proportional Hazards Models,Surgical Wound Infection,Surgical Wound Infection: epidemiology,Surgical Wound Infection: mortality,Urinary Tract Infections,Urinary Tract Infections: epidemiology,Urinary Tract Infections: mortality},
