issn = {1460-2091},
journal = {J. Antimicrob. Chemother.},
keywords = {Animals,Computational Biology,Computational Biology: methods,Escherichia coli,Escherichia coli: classification,Escherichia coli: genetics,Escherichia coli: isolation {\&} purification,Genome, Bacterial,Genotype,Microbial Sensitivity Tests,Microbial Sensitivity Tests: methods,Multilocus Sequence Typing,Salmonella typhimurium,Salmonella typhimurium: classification,Salmonella typhimurium: genetics,Salmonella typhimurium: isolation {\&} purification,Sequence Analysis, DNA,Sequence Analysis, DNA: methods,Swine,Swine: microbiology},
