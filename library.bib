journal = {Vaccine},
number = {17},
pages = {2062--2072},
pmid = {18378366},
title = {{Comparison of vaccines for induction of heterosubtypic immunity to influenza A virus: cold-adapted vaccine versus DNA prime-adenovirus boost strategies.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=18378366{\&}retmode=ref{\&}cmd=prlinks papers2://publication/doi/10.1016/j.vaccine.2008.02.047},
