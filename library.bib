isbn = {1098-5514 (Electronic)$\backslash$r0022-538X (Linking)},
issn = {1098-5514},
journal = {J. Virol.},
keywords = {Adolescent,Adult,Blood Proteins,Blood Proteins: genetics,Child,Child, Preschool,Dengue Virus,Dengue Virus: genetics,Dengue Virus: pathogenicity,Disease Progression,Enzyme-Linked Immunosorbent Assay,Female,Gene Expression Profiling,Genome, Viral,Genome, Viral: genetics,Humans,Male,Neutrophil Activation,Neutrophils,Neutrophils: metabolism,Oligonucleotide Array Sequence Analysis,Phylogeny,Polymerase Chain Reaction,Prospective Studies,RNA, Messenger,RNA, Messenger: blood,RNA, Messenger: genetics,RNA, Viral,RNA, Viral: blood,RNA, Viral: genetics,Severe Dengue,Severe Dengue: blood,Severe Dengue: genetics,Severe Dengue: virology,Vietnam,Young Adult},
