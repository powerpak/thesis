file = {:Users/powerpak/Documents/Mendeley Desktop/Minot et al. - 2012 - Hypervariable loci in the human gut virome.pdf:pdf},
isbn = {1091-6490 (Electronic)$\backslash$n0027-8424 (Linking)},
issn = {0027-8424},
journal = {Proc. Natl. Acad. Sci.},
number = {10},
pages = {3962--3966},
