file = {:Users/powerpak/Documents/Mendeley Desktop/Smee et al. - 2001 - Characterization of an influenza A (H3N2) virus resistant to the cyclopentane neuraminidase inhibitor RWJ-270201.pdf:pdf},
journal = {Antiviral Res.},
number = {3},
pages = {251--259},
title = {{Characterization of an influenza A (H3N2) virus resistant to the cyclopentane neuraminidase inhibitor RWJ-270201.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=11675142{\&}retmode=ref{\&}cmd=prlinks papers2://publication/uuid/9FCFB798-CAC0-4EEC-A847-6C5322457566},
