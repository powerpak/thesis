file = {:Users/powerpak/Documents/Mendeley Desktop/McAdam et al. - 2012 - Molecular tracing of the emergence, adaptation, and transmission of hospital-associated methicillin-resistant Sta.pdf:pdf},
issn = {1091-6490},
