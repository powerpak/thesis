pmid = {27299617},
title = {{Prescription of Long-Acting Opioids and Mortality in Patients With Chronic Noncancer Pain}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/27299617},
volume = {315},
year = {2016}
}
