journal = {Biomed Res. Int.},
keywords = {Alphavirus,Alphavirus: physiology,Animals,Arthritis, Infectious,Arthritis, Infectious: immunology,Arthritis, Infectious: metabolism,Arthritis, Infectious: pathology,Arthritis, Infectious: virology,Humans},
month = {jan},
pages = {973516},
pmid = {24069610},
title = {{Molecular mechanisms involved in the pathogenesis of alphavirus-induced arthritis.}},
