doi = {10.1073/pnas.1205576109},
file = {:Users/powerpak/Documents/Mendeley Desktop/Pascua et al. - 2012 - Virulence and transmissibility of H1N2 influenza virus in ferrets imply the continuing threat of triple-reassorta.pdf:pdf},
