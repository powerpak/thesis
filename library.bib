file = {:Users/powerpak/Documents/Mendeley Desktop/Alonso et al. - 2004 - Overexpression of the multidrug efflux pump SmeDEF impairs Stenotrophomonas maltophilia physiology.pdf:pdf},
isbn = {0305-7453 (Print)$\backslash$n0305-7453 (Linking)},
issn = {03057453},
journal = {J. Antimicrob. Chemother.},
keywords = {Antibiotic resistance,Bacterial fitness,MDR,Metabolic profiling,S. maltophilia,Virulence},
number = {3},
pages = {432--434},
pmid = {14739147},
title = {{Overexpression of the multidrug efflux pump SmeDEF impairs Stenotrophomonas maltophilia physiology}},
volume = {53},
year = {2004}
