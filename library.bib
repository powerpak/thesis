author = {Goh, Kwang-Il and Cusick, Michael E and Valle, David and Childs, Barton and Vidal, Marc and Barab{\'{a}}si, Albert-L{\'{a}}szl{\'{o}}},
doi = {10.1073/pnas.0701361104},
file = {:Users/powerpak/Documents/Mendeley Desktop/Goh et al. - 2007 - The human disease network.pdf:pdf},
journal = {Proc. Natl. Acad. Sci. U. S. A.},
number = {21},
pages = {8685--8690},
pmid = {17502601},
title = {{The human disease network.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=17502601{\&}retmode=ref{\&}cmd=prlinks papers2://publication/doi/10.1073/pnas.0701361104},
volume = {104},
year = {2007}
