journal = {J. Infect. Dis.},
number = {2},
pages = {159--167},
pmid = {16779721},
title = {{Immunization with reverse-genetics-produced H5N1 influenza vaccine protects ferrets against homologous and heterologous challenge.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=16779721{\&}retmode=ref{\&}cmd=prlinks papers2://publication/doi/10.1086/505225},
