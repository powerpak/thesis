keywords = {Electronic Health Records,Electronic Health Records: trends,Genetic Research,Genetic Research: ethics,Genome-Wide Association Study,Genomics,Genomics: ethics,Genomics: trends,Genotype,Humans,Individualized Medicine,National Human Genome Research Institute (U.S.),Phenotype,United States},
month = {oct},
number = {10},
pages = {761--71},
pmid = {23743551},
title = {{The Electronic Medical Records and Genomics (eMERGE) Network: past, present, and future.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3795928{\&}tool=pmcentrez{\&}rendertype=abstract},
