author = {Hoarau, Jean-Jacques and {Jaffar Bandjee}, Marie-Christine and {Krejbich Trotot}, Pascale and Das, Trina and Li-Pat-Yuen, Ghislaine and Dassa, B{\'{e}}reng{\`{e}}re and Denizot, M{\'{e}}lanie and Guichard, Elsa and Ribera, Anne and Henni, Tawfiq and Tallet, Frank and Moiton, Marie Pierre and Gauz{\`{e}}re, Bernard Alex and Bruniquet, Sandrine and {Jaffar Bandjee}, Za{\"{i}}noul and Morbidelli, Philippe and Martigny, G{\'{e}}rard and Jolivet, Michel and Gay, Frederick and Grandadam, Marc and Tolou, Hugues and Vieillard, Vincent and Debr{\'{e}}, Patrice and Autran, Brigitte and Gasque, Philippe},
