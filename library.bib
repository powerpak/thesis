isbn = {0883-4237 (Print)$\backslash$r0883-4237 (Linking)},
issn = {0883-4237},
journal = {Stat. Sci.},
keywords = {Observational study,and phrases,classification,expanding,however,information for researchers in-,no single source of,observational study,propensity scores,sub-,subclassif,there has been,weighting,while the field is},
number = {1},
