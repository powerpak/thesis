journal = {Proc. Natl. Acad. Sci.},
pages = {1--6},
title = {{Systematic functional regulatory assessment of disease-associated variants.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=23690573{\&}retmode=ref{\&}cmd=prlinks papers2://publication/doi/10.1073/pnas.1219099110/-/DCSupplemental},
