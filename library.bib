journal = {N. Engl. J. Med.},
number = {26},
pages = {2779--2787},
pmid = {16382064},
title = {{Trial Registration at ClinicalTrials.gov between May and October 2005.}},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed{\&}id=16382064{\&}retmode=ref{\&}cmd=prlinks papers2://publication/doi/10.1056/NEJMsa053234},
