issn = {1460-2083},
journal = {Hum. Mol. Genet.},
keywords = {Cluster Analysis,Fasting,Fasting: blood,Feeding Behavior,Feeding Behavior: physiology,Gene Expression Profiling,Gene Expression Regulation,Gene Regulatory Networks,Gene Regulatory Networks: genetics,Humans,Quantitative Trait, Heritable,ROC Curve,Time Factors},
month = {jan},
number = {1},
pages = {159--69},
pmid = {19837700},
title = {{The effect of food intake on gene expression in human peripheral blood.}},
